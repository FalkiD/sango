########################################################################
# Timing constraints
########################################################################
TIMESPEC TS_clk100 = PERIOD clk100_speed 10ns;
NET FPGA_CLK TNM = clk100_speed;
NET clk TNM = clk100_speed;

TIMESPEC TS_clk10 = PERIOD clk10_speed TS_clk100 * 10;
NET clk10 TNM = clk10_speed;

TIMESPEC TS_clk200 = PERIOD clk200_speed TS_clk100 / 2;
NET clk200 TNM = clk200_speed;

TIMESPEC TS_spi = PERIOD spi_speed 80ns;
NET SCK_IN TNM = spi_speed;
NET cfg_sck TNM = spi_speed;

INST "tg[*]/sdo_in*" TNM = "spi_speed";
TIMESPEC TS_spi_sync = FROM spi_speed TO clk100_speed TIG;

NET "reggie/dat_o*" TIG;
NET "tg*/tg_reg/dat_o*" TIG;
NET "reggie/conf*" TIG;
NET "reggie/trig_src*" TIG;
NET "reggie/irq_mask*" TIG;
NET "reggie/irq_clr*" TIG;
NET "reggie/sync*" TIG;
